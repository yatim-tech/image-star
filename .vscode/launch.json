{
    "version": "2.0.0",
    "configurations": [

        {
            "name": "Python Debugger: Validator Cycle",
            "type": "debugpy",
            "request": "launch",
            "module": "validator.cycle.main",
            "console": "integratedTerminal",
            "preLaunchTask": "start-validator-services"
        },
        {
            "name": "Python Debugger: Miner FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "miner.server:app",
                "--host",
                "0.0.0.0",
                "--port",
                "7999",
                "--env-file",
                ".1.env",
                "--reload",
                "--reload-dir",
                "miner"
            ],
            "jinja": true,
            "env": {
                "ENV": "DEV",
                "ENV_FILE": ".1.env"
            }
        },
        {
            "name": "Python: Remote Attach",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "/app"
                }
            ]
        },
        {
            "name": "Python Debugger: Manually Store Reward Functions",
            "type": "debugpy",
            "request": "launch",
            "module": "validator.db.sql.manually_add_grpo_rewards",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: Docker Evaluation",
            "type": "debugpy",
            "request": "launch",
            "module": "validator.evaluation.docker_evaluation",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: eval instruct text",
            "type": "debugpy",
            "request": "launch",
            "module": "validator.evaluation.eval_instruct_text",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: eval dpo",
            "type": "debugpy",
            "request": "launch",
            "module": "validator.evaluation.eval_dpo",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: eval grpo",
            "type": "debugpy",
            "request": "launch",
            "module": "validator.evaluation.eval_grpo",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: Task Prep Test",
            "type": "debugpy",
            "request": "launch",
            "module": "validator.tests.prepare_task_test",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: Job Handler",
            "type": "debugpy",
            "request": "launch",
            "module": "miner.logic.job_handler",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: Evaluation Utils",
            "type": "debugpy",
            "request": "launch",
            "module": "validator.evaluation.utils",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: Validator API",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "--factory",
                "validator.asgi:factory",
                "--host",
                "0.0.0.0",
                "--port",
                "9001",
                "--env-file",
                ".vali.env"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "preLaunchTask": "validator-setup",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
